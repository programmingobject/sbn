#!/usr/bin/env python3
# This file is placed in the Public Domain.


"cleanup"


# IMPORTS


import os
import sys


# DEFINES


os.environ["PATH"] += ":fin"


# UTILITY


def popen(txt):
    """run in popen"""
    for line in os.popen(txt).readlines():
        print(line.rstrip())
        sys.stdout.flush()


# RUNTIME


def main():
    """main program"""
    popen("rm -fR DEAD* build dist MANIFEST *.egg-info *.whl")
    popen("rm -fR .pytest_cache .test  __pycache__ *~ .*~")
    popen("loop . rm -fR *~")
    popen("loop . rm -fR __pycache__")


if __name__ == "__main__":
    main()
