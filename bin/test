# pylint: disable=C0114,C0115,C0116,W0703


"run pytest"


import os
import sys


def popen(txt):
    runtxt = "\n## " + txt +"\n"
    print(runtxt)
    for line in os.popen(txt).readlines():
        print(line.rstrip())
        sys.stdout.flush()


popen("python3 -m pytest")
